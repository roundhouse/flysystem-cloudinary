{
  "name": "roundhouse/flysystem-cloudinary",
  "description": "Cloudinary adapter for Flysystem.",
  "license": "MIT",
  "authors": [
    {
      "name": "Vadim Goncharov",
      "email": "vadim@owl-design.net"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-fileinfo": "*",
    "league/flysystem": "^1.0.26",
    "cloudinary/cloudinary_php": "^1.8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.3.1",
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Roundhouse\\Flysystem\\Cloudinary\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Roundhouse\\Flysystem\\Cloudinary\\Test\\": "tests/"
    },
    "files": [
      "tests/polyfill.php"
    ]
  }
}
